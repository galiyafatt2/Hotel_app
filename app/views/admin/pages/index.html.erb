<%= render 'admin/layouts/navigation' %>

<h1 align="center"> Welcome, admin </h1>

<div align="center">
  <% if @rooms %>
    <% @rooms.last(3).each do |room|%>
      <% room.images.limit(1).each do |image|%>
        <%= link_to image_tag(room.images_as_thumbnails(image)), image %>
      <%end%>
    <% end %>
  <% end %>
</div>