<%= render 'admin/layouts/navigation' %>

<h1 align="center"> Welcome, admin </h1>

<div align="center">
  <% @rooms.each do |room|%>
    <% if room.images %>
      <% room.images.limit(1).each do |image|%>
        <%= link_to image_tag(room.images_as_thumbnails(image) || "public/assets/apple-touch-icon.png"), image %>
      <%end%>
    <% end %>
  <% end %>
</div>